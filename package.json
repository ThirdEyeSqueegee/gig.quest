    "@mui/joy": "^5.0.0-beta.15",
    "@mui/material": "^5.14.18",
    "@uidotdev/usehooks": "^2.4.1",
    "axios": "^1.6.2",
    "framer-motion": "^10.16.5",
    "geolib": "^3.3.4",
    "react": "^18.2.0",
    "react-device-detect": "^2.2.3",
    "react-dom": "^18.2.0",
    "string-comparison": "^1.2.0",
